{
  "name": "promwrap",
  "version": "0.0.0-development",
  "description": "Proxy-Powered Promisification for your Pleasure",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "tap test/test-*.js --100",
    "posttest": "standard",
    "coverage": "tap test/test-*.js --coverage-report=lcov",
    "postcoverage": "codecov",
    "precommit": "npm test",
    "prepush": "npm test",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "keywords": [
    "promise",
    "wrapper",
    "promisify",
    "promises",
    "callback",
    "nodeback",
    "promisifcation"
  ],
  "author": "Mikeal Rogers <mikeal.rogers@gmail.com> (http://www.mikealrogers.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "codecov": "^2.2.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.3",
    "standard": "^10.0.2",
    "tap": "^10.7.1",
    "validate-commit-msg": "^2.14.0",
    "semantic-release": "^12.4.1",
    "travis-deploy-once": "^4.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boneskull/promwrap.git"
  }
